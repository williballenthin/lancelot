{
  "name": "lancelot-pwa",
  "version": "0.1.0",
  "description": "lancelot pwa ui",
  "scripts": {
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write .",
    "test": "env TS_NODE_PROJECT='tsconfig.testing.json' mocha -r ts-node/register tests/*",
    "build": "webpack --mode development",
    "watch": "webpack --mode=development --watch",
    "build-release": "webpack --mode production",
    "start": "webpack-dev-server --hot",
    "clean": "rm -rf public/js/*.bundle.js public/js/*.wasm public/js/*.map"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/williballenthin/lancelot"
  },
  "keywords": [],
  "author": "william.ballenthin@mandiant.com",
  "license": "Apache 2.0",
  "private": false,
  "devDependencies": {
    "@types/mocha": "^9.0.0",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-virtualized-auto-sizer": "^1.0.1",
    "@types/react-window": "^1.8.5",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "css-loader": "^6.5.1",
    "eslint": "^7.23.0",
    "mocha": "^8.3.2",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "react-refresh": "^0.10.0",
    "react-refresh-typescript": "^2.0.2",
    "style-loader": "^3.3.1",
    "ts-loader": "^8.1.0",
    "ts-mocha": "^8.0.0",
    "typescript": "^4.2.3",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^4.6.0"
  },
  "dependencies": {
    "@blueprintjs/core": "^4.0.0-beta.10",
    "@blueprintjs/icons": "^4.0.0-beta.6",
    "@blueprintjs/popover2": "^1.0.0-beta.10",
    "@blueprintjs/select": "^4.0.0-beta.10",
    "@blueprintjs/table": "^4.0.0-beta.10",
    "@popperjs/core": "^2.11.0",
    "@reduxjs/toolkit": "^1.6.2",
    "assert": "^2.0.0",
    "console-browserify": "^1.2.0",
    "rc-dock": "^3.2.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-virtualized-auto-sizer": "^1.0.6",
    "react-window": "^1.8.6"
  }
